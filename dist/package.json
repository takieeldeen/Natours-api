{
  "name": "natours",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "build": "tsc && tscp",
    "start": "node dist/server.js",
    "test": "node dist/app.js && tsc",
    "dev": "NODE_ENV=development concurrently \"tsc -w & tscp -w\" \"nodemon dist/server.js\"",
    "prod": "NODE_ENV=production concurrently \"tsc -w & tscp -w\" \"nodemon dist/server.js\"",
    "debug": "ndb dist/server.js",
    "watch:ts": "parcel watch ./public/scripts/index.ts ./public/scripts bundle.ts"
  },
