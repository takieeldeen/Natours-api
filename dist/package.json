    "concurrently": "^9.0.1",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "nodemailer": "^6.9.16",
    "nodemon": "^3.1.7",
    "parcel": "^2.13.3",
    "parcel-bundler": "^1.12.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
